<%= form_for @book, :url => books_path do |f| %>
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %>

    <%= f.label :description, "Description" %>
    <%= f.text_area :description %>

    <%= f.label :isbn, "ISBN" %>
    <%= f.text_field :isbn %>

    </br>
    <%= f.label :publish_date, "出版日期" %>
    <%= f.date_select :publish_date %>
    </br>
    <%= f.label :onBoard_datetime, "上架日期" %>
    <%= f.datetime_select :onBoard_datetime %>

    <%= f.submit "Create" %>
<% end %>
